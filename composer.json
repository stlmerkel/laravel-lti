{
  "name": "stlmerkel/laravel-lti",
  "description": "Laravel package wrapper for the LTI-PHP tool provider.",
  "keywords": ["LTI", "Laravel", "ToolProvider", "Learning Tools Interoperability"],
  "homepage": "https://github.com/stlmerkel/laravel-lti", 
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Stephen P Vickers",
      "email": "stephen@spvsoftwareproducts.com"
    },
    {
      "name": "Brian Merkel",
      "email": "stlmerkel@protonmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "firebase/php-jwt": "^6.8"
  },
  "autoload": {
    "psr-4": {
      "Stlmerkel\\LaravelLTI\\": "src/"
    },
    "classmap": [
      "src/Core"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Stlmerkel\\LaravelLTI\\LtiServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}